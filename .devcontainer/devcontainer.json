{
    "name": "Python 3 & PostgreSQL",
    "dockerComposeFile": [
        "../docker-compose.yml",
        "./docker-compose.yml"
    ],
    "service": "web",
    "workspaceFolder": "/app",
    "settings": {
        "terminal.integrated.shell.linux": "/bin/bash",
        "python.pythonPath": "/usr/local/bin/python",
        "python.envFile": "${localWorkspaceFolder}/.env"
    },
    "extensions": [
        "ms-python.python"
    ],
}